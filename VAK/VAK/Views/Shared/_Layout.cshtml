<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/Main.css")
</head>
<body>
    @RenderBody()
    @Scripts.Render(Url.Content("~/Scripts/lib/angular.js"))
    @Scripts.Render(Url.Content("~/Scripts/lib/jquery-1.8.2.js"))
	@Scripts.Render(Url.Content("~/Scripts/lib/jquery-ui-1.8.24.js"))
	@Scripts.Render(Url.Content("~/Scripts/app/test.js"))
    @RenderSection("scripts", required: false)
</body>
</html>
